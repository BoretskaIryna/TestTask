<p id="notice"><%= notice %></p>

<h4>Members</h4>

<table class="table">
  <thead>
  <tr>
    <th>Collaborator</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @members.each do |member| %>
      <tr>
        <td><%= member.collaborator.name%></td>
        <% if current_user.project_manager%>
          <td><%= link_to 'Destroy', member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>
      </tr>
  <% end %>
  </tbody>
</table>

<br>